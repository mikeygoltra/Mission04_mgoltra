@{
}


@model gradeCalculatorModel;
<!-- grade calculator-->
<h2>&nbspGrade Calculator:</h2>
<form method="post">
    <label>&nbspAssignments:</label>
    <input id="assignmentsNum" asp-for="Assignments" /><br />
    <label>&nbspGroup Project:</label>
    <input id="groupProjectNum" asp-for="GroupProjects" /><br />
    <label>&nbspQuizzes:</label>
    <input id="quizzesNum" asp-for="Quizzes" /><br />
    <label>&nbspMidterm Exam:</label>
    <input id="midtermExamNum" asp-for="Midterm" /><br />
    <label>&nbspFinal Exam:</label>
    <input id="finalExamNum" asp-for="Final" /><br />
    <label>&nbspINTEX:</label>
    <input id="intexNum" asp-for="INTEX" /><br />
    <input id="calculate" type="submit" value="Calculate" />

    <br />
    <br />
    <div asp-validation-summary="All">

    </div>
</form>
<p id="test"></p>
<!-- jquery-->
<script src="~/lib/jquery/jquery-3.6.3.min.js"></script>

<script src="~/js/calculator.js"></script>